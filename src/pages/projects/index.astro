---
import BaseLayout from '../../layouts/BaseLayout.astro';
import ProjectList from '../../components/ProjectList.astro';
import { projects } from './projects';

---

<BaseLayout
	title="All My Projects"
	description="All my project portfolio from real projects to open source projects."
>
  <div class='container'>
    <div class="mb-10">
      <h1 class="text-3xl font-bold mb-2 text-gruvbox-yellow">All My Projects</h1>
      <p class="text-gruvbox-fg2 text-lg">These are the projects i work on in my free time</p>
    </div>
    <ProjectList projects={projects}/>
  </div>
</BaseLayout>
