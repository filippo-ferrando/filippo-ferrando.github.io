---
import BaseLayout from "../layouts/BaseLayout.astro";
---

<BaseLayout>

    <script>
			let rnd = Math.floor(Math.random() * (5 + 1));
			let chosen = "../public/404-image/" + rnd + ".gif";
            document.getElementById("iframe-404").src = chosen;
    </script>
    <div class="pb-12 mt-5">
        <div class="text-5xl font-bold">Page Not Found 🤷🏽</div>
    </div>
    <iframe src="" width="1400" height="350" id="iframe-404" class="gif"></iframe>
    

        
        
</BaseLayout>